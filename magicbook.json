{
   "title":"The Nature of Code",
   "destination":"build/:build",
   "files":[
      "chapters/test.html"
   ],
   "addPlugins":[
     "magicbook-codesplit",
     "magicbook-katex"
   ],
   "builds":[
      {
         "format":"pdf",
         "prince" : {
            "log":"errors.txt",
            "timeout" : 300000
