<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<title>Document</title>
		<style type="text/css" media="screen">
			#container {
				width: 800px;
				margin: 0 auto;
			}
			.breakout_box {
				background-color: #D7D7D7;
				padding: 20px;
			}
			canvas {
				border: 2px solid red;
			}
			pre {
				width: 100%;
				overflow: hidden;
			}
		</style>
	</head>
	<body>
		<div id="container">
			<% if toc %>
				<%= toc %>
			<% end %>
			<%= document.element.standard_render(views) %>
		</div>
	</body>
</html>